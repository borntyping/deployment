ControlMaster auto
ControlPath /tmp/ssh-%i-%r@%h:%p
ControlPersist yes
GSSAPIAuthentication no
ServerAliveInterval 5
StrictHostKeyChecking no
TCPKeepAlive yes

Host github.com
    User git
    IdentitiesOnly yes
    IdentityFile "~/.ssh/keys/id_rsa_github"

Host gitlab.com
    User git
    IdentitiesOnly yes
    IdentityFile "~/.ssh/keys/id_rsa_gitlab"
